{
	"name": "obsidian-rdf-tools",
	"version": "1.0.0",
	"description": "RDF Tools for Obsidian - Work with RDF data and SPARQL queries directly in your vault",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"format": "prettier --write src/**/*.ts",
		"format:check": "prettier --check src/**/*.ts",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"check-all": "npm run format && npm run lint:fix && npm run typecheck"
	},
	"keywords": [],
	"author": "shad",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@comunica/query-sparql": "^4.3.0",
		"n3": "^1.26.0"
	}
}
